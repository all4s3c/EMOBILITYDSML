      <validationSet>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UC1_CSMS_Software" level="ERROR" message="aql:self.oclAsType(emobility::CSMS).name+' is vulnerable to UC1_CSMS_Software'" targetClass="emobility::CSMS">
          <audits auditExpression="aql:(self.oclAsType(emobility::CSMS).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Secure_Software_Developement_Process)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::CSMS).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Logging_access_and_actions)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::CSMS).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Authentication)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::CSMS).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Access_Control)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::CSMS).server.oclAsType(emobility::Server).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Hardening)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::CSMS).server.oclAsType(emobility::Server).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Patch_Management)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UC2_EVSE_Physical" level="ERROR" message="aql:self.oclAsType(emobility::EVSE).name+' is vulnerable to UC2_EVSE_Physical'" targetClass="emobility::EVSE">
          <audits auditExpression="aql:(self.oclAsType(emobility::EVSE).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Locking_mechanisms)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::EVSE).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Door_sensors)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::EVSE).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Tamper_evident_seals)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::EVSE).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Inspection_of_seals_and_Intrenal_material)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UC3_EVSE_Internet" level="ERROR" message="aql:self.oclAsType(emobility::EVSE).name+' is vulnerable to UC3_EVSE_Internet'" targetClass="emobility::EVSE">
          <audits auditExpression="aql:(self.oclAsType(emobility::EVSE).isExposed=true  implies self.oclAsType(emobility::EVSE).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Patch_Management)"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::EVSE).isExposed=true  implies self.oclAsType(emobility::EVSE).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Hardening)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UC4_MFA" level="ERROR" message="aql:self.oclAsType(emobility::EVSE).name+' is vulnerable to UC4_MFA'" targetClass="emobility::EVSE">
          <audits auditExpression="aql:(self.oclAsType(emobility::EVSE).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Authentication_OTP )"/>
          <audits auditExpression="aql:(self.oclAsType(emobility::EVSE).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::RFID_Card or sec.type=emobility::SecurityControlType::Authentication_App )"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UC5_Communication" level="ERROR" message="aql:self.oclAsType(emobility::Communication).name+' is vulnerable to sniffing'" targetClass="emobility::Communication">
          <audits auditExpression="aql:(self.oclAsType(emobility::Communication).Confidentiality=emobility::CIA_Level::L3  implies self.oclAsType(emobility::Communication).securitycontrol->exists(sec | sec.type=emobility::SecurityControlType::Encryption )"/>
        </ownedRules>
      </validationSet>
